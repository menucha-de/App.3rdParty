<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="org/jvnet/jaxb2_commons" module="jaxb2-basics-tools" status="integration" default="true" />

	<configurations>
		<conf name="default" visibility="public" />
		<conf name="doc" visibility="public" />
		<conf name="src" visibility="public" />
	</configurations>

	<publications>
		<artifact name="jaxb2-basics-tools" type="jar" ext="jar" conf="default" />
		<artifact name="jaxb2-basics-tools-javadoc" type="jar" ext="jar" conf="doc" />
		<artifact name="jaxb2-basics-tools-sources" type="jar" ext="jar" conf="src" />
	</publications>

	<dependencies>
		<dependency org="org/jvnet/jaxb2_commons" name="jaxb2-basics-runtime" rev="${jaxb2-basics.version}" conf="default" />
	</dependencies>

</ivy-module>