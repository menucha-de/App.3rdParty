<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="org/jboss/resteasy" module="resteasy-multipart-provider" status="integration" default="true" />

	<configurations>
		<conf name="default" />
		<conf name="src" />
	</configurations>

	<publications>
		<artifact name="resteasy-multipart-provider" conf="default" />
		<artifact name="resteasy-multipart-provider-sources" conf="src" />
	</publications>

	<dependencies>
		<dependency org="javax/ws/rs" name="javax.ws.rs-api" rev="${javax.ws.rs-api.version}" conf="default" />
		<dependency org="org/jboss/resteasy" name="resteasy-jaxrs" rev="${publish.current.version}" conf="default" />
		<dependency org="org/apache/james" name="apache-mime4j" rev="${mime4j.version}" conf="default" />
	</dependencies>

</ivy-module>
