<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="org/jibx" module="jibx-schema" status="integration" default="true" />

	<configurations>
		<conf name="default" visibility="public" />
		<conf name="doc" visibility="public" />
		<conf name="src" visibility="public" />
	</configurations>

	<publications>
		<artifact name="jibx-schema" type="jar" ext="jar" conf="default" />
		<artifact name="jibx-schema-javadoc" type="jar" ext="jar" conf="doc" />
		<artifact name="jibx-schema-sources" type="jar" ext="jar" conf="src" />
	</publications>

	<dependencies>
		<dependency org="org/jibx" name="jibx-bind" rev="${jibx.version}" conf="default" />
		<dependency org="org/jibx" name="jibx-extras" rev="${jibx.version}" conf="default" />
	</dependencies>

</ivy-module>