<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="io/netty" module="netty-resolver" status="integration" default="true" />

	<configurations>
		<conf name="default" />
		<conf name="module" />
		<conf name="doc" />
		<conf name="src" />
	</configurations>

	<publications>
		<artifact name="netty-resolver" conf="default,module" />
		<artifact name="netty-resolver-javadoc" conf="doc" />
		<artifact name="netty-resolver-sources" conf="src" />
	</publications>

	<dependencies>
		<dependency org="io/netty" name="netty-common" rev="${netty.version}" conf="default;module->default" />
	</dependencies>

</ivy-module>