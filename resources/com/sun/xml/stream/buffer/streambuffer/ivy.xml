<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="com/sun/xml/stream/buffer" module="streambuffer" status="integration" default="true" />

	<configurations>
		<conf name="default" visibility="public" />
		<conf name="doc" visibility="public" />
		<conf name="src" visibility="public" />
	</configurations>

	<publications>
		<artifact name="streambuffer" type="jar" ext="jar" conf="default" />
		<artifact name="streambuffer-javadoc" type="jar" ext="jar" conf="doc" />
		<artifact name="streambuffer-sources" type="jar" ext="jar" conf="src" />
	</publications>

	<dependencies>
		<dependency org="org/jvnet/staxex" name="stax-ex" rev="${stax-ex.version}" conf="default" />
	</dependencies>

</ivy-module>