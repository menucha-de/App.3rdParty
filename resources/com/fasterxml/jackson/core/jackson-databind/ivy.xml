<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="com/fasterxml/jackson/core" module="jackson-databind" status="integration" default="true" />

	<configurations>
		<conf name="default" />
		<conf name="module" />
		<conf name="src" />
	</configurations>

	<publications>
		<artifact name="jackson-databind" conf="default,module" />
		<artifact name="jackson-databind-sources" conf="src" />
	</publications>

	<dependencies>
		<dependency org="com/fasterxml/jackson/core" name="jackson-core" rev="${jackson.version}" conf="default" />
		<dependency org="com/fasterxml/jackson/core" name="jackson-annotations" rev="${jackson.version}" conf="default" />
	</dependencies>

</ivy-module>