<?xml version="1.0" encoding="utf-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="com/google/gwt/inject" module="gin" status="integration" default="true" />

	<configurations>
		<conf name="default" visibility="public" />
		<conf name="doc" visibility="public" />
		<conf name="src" visibility="public" />
	</configurations>

	<publications>
		<artifact name="gin" type="jar" ext="jar" conf="default" />
		<artifact name="gin-javadoc" type="jar" ext="jar" conf="doc" />
		<artifact name="gin-sources" type="jar" ext="jar" conf="src" />
	</publications>

	<dependencies>
		<dependency org="com/google/inject" name="guice" rev="${com.google.guice.version}" conf="default" />
		<dependency org="com/google/inject/extensions" name="guice-assistedinject" rev="${com.google.guice.version}" conf="default" />
	</dependencies>
</ivy-module>